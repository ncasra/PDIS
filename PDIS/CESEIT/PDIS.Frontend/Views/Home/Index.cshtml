<div class="row delivery-details-row">
    <div class="col-sm-6">
        <div class="form-group">
            <label for="inputFrom">Fra:</label>
            <select class="form-control" id="inputFrom">
                <option>Sierra Leone</option>
                <option>Noget andet</option>
            </select>
        </div>
        <div class="form-group">
            <label for="inputTo">Til:</label>
            <select class="form-control" id="inputTo">
                <option>Sierra Leone</option>
                <option>Noget Andet</option>
            </select>
        </div>
        <div class="form-group">
            <label for="inputDate">Afgangsdato:</label>
            <input type="date" class="form-control" id="inputDate"/>
        </div>
    </div>
    <div class="col-sm-6">
        <div class="form-group">
            <label for="inputWeight">Vægt(kg):</label>
            <input type="number" class="form-control" id="inputWeight"/>
        </div>
        <div class="form-group">
            <label for="inputType">Type</label>
            <select class="form-control" id="inputType">
                <option>TODO</option>
                <option>Add</option>
                <option>Types</option>
            </select>
        </div>
        <div class="form-group">
            <label for="inputDimension">Længste dimension(cm):</label>
            <input type="number" class="form-control" id="inputDimension"/>
        </div>
        <button type="submit" class="btn btn-default btn-primary">BEREGN</button>
    </div>
</div>
<div class="row">
    <div class="col-xs-12">
        <div class="delivery-options">
            <div class="delivery-option" data-toggle="modal" data-target="#orderModal">
                <h2>Hurtigste</h2>
                <h4>TODO - TODO - TODO</h4>
                <div class="delivery-information">
                    <div>
                        <span><strong>Leveringsdato: </strong></span>
                        <span>02-09-2017</span>
                    </div>
                    <div>
                        <span><strong>Pris: </strong></span>
                        <span>3000</span>
                    </div>
                </div>
            </div>
            <div class="delivery-option" data-toggle="modal" data-target="#orderModal">
                <h2>Billigste</h2>
                <h4 class="route">TODO - TODO</h4>
                <div class="delivery-information">
                    <div>
                        <span><strong>Leveringsdato: </strong></span>
                        <span>05-09-2017</span>                        
                    </div>
                    <div>
                        <span><strong>Pris: </strong></span>
                        <span>500</span>                        
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="modal fade" id="orderModal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h2 class="modal-title">Book transport</h2>
            </div>
            <div class="modal-body">
                <div class="transport-details">
                    <h3>TODOste</h3>
                    <h4>TODO - TODO - TODO</h4>
                    <div>Leveringsdato:</div>
                    <div>Pris:</div>
                    <div>Type:</div>
                </div>
                <div class="order-details">
                    <div class="form-group">
                        <label for="inputCustomerNo">Kundenummer:</label>
                        <input type="text" class="form-control" id="inputCustomerNo"/>
                    </div>
                    <div class="form-group">
                        <label for="inputDiscount">Rabat (%):</label>
                        <input type="number" class="form-control" id="inputDiscount" />
                    </div>
                    <div>Pris efter rabat: </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">ANNULLER</button>
                <button type="button" class="btn btn-primary">OPRET</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->